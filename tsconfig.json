{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@stackbit/types": [
                "node_modules/@stackbit/types"
            ],
            "@stackbit/cms-git": [
                "node_modules/@stackbit/cms-git"
            ]
        },
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "forceConsistentCasingInFileNames": true
    }
}